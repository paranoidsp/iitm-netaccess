{
  "manifest_version": 2,

  "name": "NetAccess",
  "description": "An extension for netaccess.iitm.ac.in",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
     "<all_urls>"  ,
     "http://www.wikipedia.org/",
     "tabs"
  ],
  "browser_action": {
    "default_icon": "icon-new.png",
    "default_popup": "popup.html"
  }
}
